class Spy4 
{
	public static void main(String[] args) 
	{
		int count=0;
		for (int i=1;i<=1000 ;i++ )
		{
			int temp=i;
			int sod=0;
			int pod=1;
			while (temp!=0)
			{
				int digit=temp%10;
				sod=sod+digit;
				pod=pod*digit;
				temp=temp/10;
			}
			if (sod==pod)
			{
				count++;
			}
		}
				System.out.println(count);
				if (count!=0)
				{
					int temp1=count;
					int rev=0;
					int digit1=temp1%10;
					rev=rev*10+digit1;
					temp1=temp1/10;

				}
				if (rev==count)
				{
					System.out.println("the count value is palindrome");
				}
				else
		{
					System.out.println("the count value not a palindrome");
		}
		}
		
	}

