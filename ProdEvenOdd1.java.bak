import java.util.Scanner;
class ProdEvenOdd 
{
	public static void main(String[] args) 
	{
		Scanner sc=new Scanner(System.in);
		System.out.println("enter m value");
		int m=sc.nextInt();
		System.out.println("enter n value");
		int n=sc.nextInt();
		int prod=1;
		for (int i=m;i<=n ;i++ )
		{
			prod=prod*i;
			System.out.println("product of num from m to n:"+prod);
		}
		if (prod%2==0)
		{
			System.out.println("product of num is even:"+prod);
		}
		else
		{
			System.out.println("product of num is odd:"+prod);
		}
	}
}
